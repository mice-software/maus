
Configuration TestGeometry
{
    Dimensions 1 1 20 m
    PropertyString Material Galactic
    PropertyDouble G4StepMax 100.0 mm
    Substitution $Test 1e-3 
    Module CenterCoil
    {
      Volume None
      Position 0.0 0.0 0. m
      Rotation 0.0 0.0 0.0 degree
      ScaleFactor +152.44
      // Field
      PropertyString FieldType      Solenoid
      PropertyString FieldMapMode   Read
      PropertyString FileName       ${MAUS_ROOT_DIR}/tmp/center.fld
      PropertyDouble CurrentDensity 1.
      PropertyDouble Length         2000. mm
      PropertyDouble Thickness      22.1
      PropertyDouble InnerRadius    258.
      PropertyString FieldName      CC
    }
    Module QuadTypeQC
    {
      Volume None
      PropertyHep3Vector Position 0.0 0.0 2.0 m
      PropertyHep3Vector Rotation 0.0 0.0 0.0 degree
      PropertyString FieldType           Multipole
      PropertyInt    Pole                2 //Quad field
      PropertyInt    MaxEndPole          3 //Simulate end field up to sextupole order
      PropertyDouble FieldStrength       $Test   T
      PropertyDouble Height              1.0     m
      PropertyDouble Width               1.0     m     
      PropertyDouble Length              1.0     m
      PropertyDouble EndLength           0.1     m
      PropertyDouble CentreLength        0.5     m
      PropertyString EndFieldType        Tanh
      PropertyDouble ScaleFactor         +1
      PropertyString FieldName           Q2
    }
    Module QuadTypeQC
    {
      Volume None
      PropertyHep3Vector Position 0.0 0.0 3.0 m
      PropertyHep3Vector Rotation 0.0 0.0 0.0 degree
      PropertyString FieldType           Multipole
      PropertyInt    Pole                2 //Quad field
      PropertyInt    MaxEndPole          3 //Simulate end field up to sextupole order
      PropertyDouble FieldStrength       $Test     T //normalise to 1 T/m
      PropertyDouble Height              1.0     m
      PropertyDouble Width               1.0     m     
      PropertyDouble Length              1.0     m
      PropertyDouble EndLength           0.1     m
      PropertyDouble CentreLength        0.5     m
      PropertyString EndFieldType        Tanh
      PropertyDouble ScaleFactor         -1
      PropertyString FieldName           Q3
    }
    Module QuadTypeQC
    {
      Volume None
      PropertyHep3Vector Position 0.0 0.0 4.0 m
      PropertyHep3Vector Rotation 0.0 0.0 0.0 degree
      PropertyString FieldType           Multipole
      PropertyInt    Pole                2 //Quad field
      PropertyInt    MaxEndPole          3 //Simulate end field up to sextupole order
      PropertyDouble FieldStrength       $Test     T //normalise to 1 T/m
      PropertyDouble Height              1.0     m
      PropertyDouble Width               1.0     m     
