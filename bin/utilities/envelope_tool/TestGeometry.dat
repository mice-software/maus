      PropertyDouble EndLength           0.1     m
      PropertyDouble CentreLength        0.5     m
      PropertyString EndFieldType        Tanh
      PropertyDouble ScaleFactor         +1
      PropertyString FieldName           Q4
    }
    Module QuadTypeQC
    {
      Volume None
      PropertyHep3Vector Position 0.0 0.0 5.0 m
      PropertyHep3Vector Rotation 0.0 0.0 0.0 degree
      PropertyString FieldType           Multipole
      PropertyInt    Pole                2 //Quad field
      PropertyInt    MaxEndPole          3 //Simulate end field up to sextupole order
      PropertyDouble FieldStrength       $Test     T //normalise to 1 T/m
      PropertyDouble Height              1.0     m
      PropertyDouble Width               1.0     m     
      PropertyDouble Length              1.0     m
      PropertyDouble EndLength           0.1     m
      PropertyDouble CentreLength        0.5     m
      PropertyString EndFieldType        Tanh
      PropertyDouble ScaleFactor         -1
      PropertyString FieldName           Q5
    }
    Module QuadTypeQC
    {
      Volume None
      PropertyHep3Vector Position 0.0 0.0 6.0 m
