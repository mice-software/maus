  Module Tests/line.dat
  {
    Position 0.0 0.0 0.0 cm
    Rotation 90.0 0.0 0.0 degree
  }
  Module Tests/line.dat
  {
    Position 0.0 40.0 0.0 cm
    Rotation 0.0 90.0 0.0 degree
  }
}
